{
    "name": "react-animated-tree-v2",
    "version": "1.0.6",
    "description": "Animate React with ease",
    "main": "dist/react-animated-tree-v2.cjs.js",
    "module": "dist/react-animated-tree-v2.es.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src",
        "API.md"
    ],
    "sideEffects": false,
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "rollup -c rollup.config.js --bundleConfigAsCjs",
        "test": "echo will do",
        "precommit": "lint-staged"
    },
    "lint-staged": {
        "*.{js,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "prettier": {
        "semi": true,
        "trailingComma": "es5",
        "max-len": 120,
        "printWidth": 120,
        "singleQuote": false,
        "arrowParens": "always",
        "proseWrap": "preserve",
        "tabWidth": 4,
        "bracketSpacing": true
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/adityasonel/react-animated-tree-v2.git"
    },
    "keywords": [
        "react",
        "motion",
        "animated",
        "animation",
        "spring",
        "tree"
    ],
    "author": "Aditya Sonel",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/adityasonel/react-animated-tree-v2/issues"
    },
    "homepage": "https://github.com/adityasonel/react-animated-tree-v2#readme",
    "devDependencies": {
        "@babel/core": "^7.13.0",
        "@babel/preset-env": "^7.1.6",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^25.0.2",
        "@rollup/plugin-node-resolve": "^15.1.0",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "react": "^16.8.0",
        "react-dom": "^16.8.0",
        "react-spring": "8.0.27",
        "rimraf": "^5.0.1",
        "rollup": "^3.26.2",
        "rollup-plugin-typescript2": "^0.35.0",
        "tslib": "^2.6.0",
        "typescript": "^5.1.6"
    }
}
