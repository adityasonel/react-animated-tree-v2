        "babel-core": "^6.26.3",
        "babel-jest": "^29.3.1",
        "husky": "^8.0.2",
        "lint-staged": "^13.0.3",
        "rimraf": "^3.0.2",
        "rollup": "^3.2.5",
        "rollup-plugin-size-snapshot": "^0.12.0",
        "rollup-plugin-uglify": "^6.0.4"
    },
