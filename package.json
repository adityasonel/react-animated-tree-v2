    "description": "Animate React with ease",
    "main": "dist/react-animated-tree-v2.cjs.js",
    "module": "dist/react-animated-tree-v2.es.js",
    "typings": "index.d.ts",
    "files": [
        "dist",
        "src",
        "API.md",
        "index.d.ts"
    ],
